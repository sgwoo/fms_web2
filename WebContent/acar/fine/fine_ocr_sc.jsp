		fm.car_no.value = car_no;
		fm.gubun1.value = gubun1;
		fm.seq.value = seq;
		fm.file_name.value = file_name;
		fm.save_folder.value = save_folder;
		fm.action = 'fine_ocr_reg_frame.jsp'
		fm.target = 'd_content';
		fm.submit();
	}
	
</script>
