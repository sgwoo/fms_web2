	//엑셀 
	function pop_excel(){
		var fm = i_no.document.form1;	
		var len=fm.elements.length;
		var cnt=0;
		var idnum="";
		for(var i=0 ; i<len ; i++){
			var ck=fm.elements[i];		
			if(ck.name == "ch_l_cd"){		
				if(ck.checked == true){
					cnt++;
					idnum=ck.value;
				}
			}
		}	
		if(cnt == 0){
		 	alert("엑셀로 출력할 계약을 선택하세요.");
			return;
		}	
		fm.target = "_blank";
		fm.action = "/acar/forfeit_mng/popup_excel.jsp";
		fm.submit();
	}	
	
	//퇴사자영업담당 대손처리
	function select_fine(){
		var fm = i_no.document.form1;	
		var len=fm.elements.length;
		var cnt=0;
		var idnum="";
		for(var i=0 ; i<len ; i++){
			var ck=fm.elements[i];		
			if(ck.name == "ch_l_cd"){		
				if(ck.checked == true){
					cnt++;
					idnum=ck.value;
				}
			}
		}	
		if(cnt == 0){
		 	alert("엑셀로 출력할 계약을 선택하세요.");
			return;
		}
		if(confirm('대손처리 하시겠습니까?')){			
			fm.target = "_blank";
			fm.action = "bad_find_sanction.jsp";
			fm.submit();	
		}
	}	

