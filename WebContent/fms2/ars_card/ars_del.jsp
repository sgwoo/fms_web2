  <input type='hidden' name='end_dt' 	value='<%=end_dt%>'>
  
</form>
<script language='javascript'>
	var fm = document.form1;

	
<%	if(!flag1){%>
		alert('에러입니다.\n\n확인하십시오');
<%	}else{%>		
		alert('삭제되었습니다.');
<%	}%>

	fm.action = 'ars_card_frame.jsp';	
	fm.target = 'd_content';
	fm.submit();

</script>
