	}		
	//통합메모
	function view_memo2(m_id, l_cd)
	{
		window.open("/acar/settle_acc/memo_frame_s.jsp?auth_rw=<%=auth_rw%>&m_id="+m_id+"&l_cd="+l_cd, "MEMO", "left=50, top=50, width=800, height=600");
	}
	//대여료메모
	function view_memo(m_id, l_cd)
