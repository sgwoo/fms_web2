		fm.rent_mng_id.value = m_id;
		fm.rent_l_cd.value = l_cd;
		if(car_st == '예비') fm.c_st.value = 'car';
		fm.target ='d_content';
		if(use_yn == '')		fm.action = 'lc_b_s.jsp';
		else					fm.action = 'lc_c_frame.jsp';
		fm.submit();
	}
