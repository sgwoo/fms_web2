	}
	
	function pay_coupon_print(pay_dt, off_nm){
		window.open("print_coupon_list.jsp?pay_dt="+pay_dt+"&off_nm="+off_nm, "PAY_COUPON_LIST", "left=100, top=100, width=600, height=600, resizable=yes, scrollbars=yes, status=yes");					
	}

	//지급처리
	function select_coupons(){
		var fm = inner.document.form1;	
		var len=fm.elements.length;
		var cnt=0;
		var idnum="";
		for(var i=0 ; i<len ; i++){
			var ck=fm.elements[i];		
			if(ck.name == "ch_cd"){		
				if(ck.checked == true){
					cnt++;
					idnum=ck.value;
				}
			}
		}	
		if(cnt == 0){
		 	alert("지급처리할 쿠폰을 선택하세요.");
			return;
		}	
		
		window.open('about:blank', "COUPONPAY", "left=0, top=0, width=850, height=400, scrollbars=yes, status=yes, resizable=yes");		
		
		fm.target = "COUPONPAY";
		fm.action = "pur_coupon_pay.jsp";
		fm.submit();	
	}
//-->
</script>
